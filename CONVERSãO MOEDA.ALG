Algoritmo "conversão moeda"

Var

reais, ale, dolar, libra: real


Inicio

Escreva (" Quantos reais você tem?  ")
leia (reais)

ale <- reais / 2
dolar <- reais / 1.8
libra <- reais / 3.57

escreval ("Você possui em: ")
escreval ("Marcos Alemães,",ale:2:2)
escreval ("Dollar,",dolar:2:2)
escreval ("Libras,",libra:2:2)

Fimalgoritmo