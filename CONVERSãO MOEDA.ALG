Algoritmo "conversão moeda"

Var

reais, ale, dolar, libra: real


Inicio

Escreval (" Quantos reais você tem? ")
leia (reais)

ale <- reais * 2
dolar <- reais * 1.8
libra <- reais * 3.57
escreval ("Marcos Alemães,",ale,",00.")
escreval ("Dollar,",dolar,",00.")
escreval ("Libras,",libra,",00.")

Fimalgoritmo